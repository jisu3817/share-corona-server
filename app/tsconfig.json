{
  "compilerOptions": {
    "target": "es5",                                  
    "module": "commonjs", 
    // "moduleResolution": "node", 
    "outDir": "./dist",                                                          
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,         
    "skipLibCheck": true,
    // "baseUrl": ".",
    // "paths": {
      // "*": ["node_modules/*"]
    // },         
    // "downlevelIteration": true,   
    // "noImplicitAny": false,
    "types": ["jest", "node", "@types/jest"],     
  },
  "compileOnSave": true,
  "buildOnSave": true,
  "include": ["./**/*"],
  "exclude": ["node_modules"],
  "typeRoots": ["./src/@types/", "./node_modules/@types"]
}
